        },
        {
          "row": 165,
          "command": "print",
          "args": [
            "{player_name}",
            "Well, I know we may have broken our oath\\nof peace...",
            "VO_CHR_MAINSTORY_02_09_0025"
          ],
          "end": 1
        },
        {
          "row": 167,
          "command": "c_face",
          "args": [
