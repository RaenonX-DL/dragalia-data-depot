          "end": 1
        },
        {
          "row": 68,
          "command": "play_sound",
          "args": [
            "VO_CHR_MAINSTORY_17_01_0012"
          ],
          "end": 1
        },
        {
          "row": 69,
          "command": "print",
          "args": [
