{
  "paths": {
	"decrypter": {
      "dir": "lib/Decrypter",
      "dll": "Decrypt.dll"
    },
    "assetStudio": {
      "configDir": "as",
      "cliDir": "lib/AssetStudio",
      "cliExe": "AssetStudioCLI.exe"
    },
    "audio": {
      "regExp": ".^",
      "exportDir": "audio",
      "vgmStreamDir": "lib/vgmstream",
      "vgmStreamExe": "vgmstream.exe"
    },
    "index": {
      "indexPath": "index.json"
    },
    "cdnDir": ".exclude/cdn",
    "exportDir": "..",
    "incrDir": "../incr",
    "tempDir": ".exclude/temp"
  },
  "single": [
    {
      "name": "master",
      "config": "master.json",
      "multiLocale": true
    },
    {
      "name": "actions",
      "config": "actions.json",
      "multiLocale": false
    },
    {
      "name": "images/ingame/ui",
      "config": "ui.json",
      "multiLocale": true
    }
  ],
  "multi": [
    {
      "regExp": "images/icon/",
      "config": "icons.json",
      "skipExists": true,
      "multiLocale": true
    },
    {
      "regExp": "images/outgame/unitdetail/(amulet|chara|dragon)",
      "config": "unitdetail.json",
      "skipExists": true,
      "multiLocale": false
    },
    {
      "regExp": "((characters|dragon)/motion/|meshes/dragon)",
      "config": "animation.json",
      "skipExists": true,
      "multiLocale": false
    },
    {
      "regExp": "story/(unit|castle|quest)story/",
      "config": "story.json",
      "skipExists": true,
      "multiLocale": true
    }
  ]
}